<aura:component controller = "AccountController">
    <aura:attribute name = "accounts" type = "Account[]"/>
    <aura:handler name = "init" value = "{!this}" action = "{!c.doinit}"/>
    <ul>
    <aura:iteration items = "{!v.accounts}" var = "R">
       
        <c:AccountListItem account = "{!R}"/>
        
        </aura:iteration>
    </ul>
</aura:component>